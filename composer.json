{
    "name": "odirleiborgert/borgert-cms",
    "description": "Borgert CMS - Laravel Framework.",
    "keywords": ["framework", "cms", "borgert", "rocket", "content management system", "open source", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Odirlei Borgert",
            "email": "odirleiborgert@gmail.com"
        }
    ],
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "5.6.*",
        "doctrine/dbal": "^2.6",
        "thomaswelton/laravel-gravatar": "^1.1",
        "kyslik/column-sortable": "5.6.*",
        "laravel/passport": "^5.0",
        "rap2hpoutre/laravel-log-viewer": "^0.11.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "1.0.*",
        "phpunit/phpunit": "~7.0",
        "filp/whoops": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/helper.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
